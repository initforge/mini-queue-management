# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Database Configuration
DATABASE_URL=postgresql://admin:password@db:5432/queue_managment
POSTGRES_DB=queue_managment
POSTGRES_USER=admin
POSTGRES_PASSWORD=password

# Redis Configuration
REDIS_URL=redis://redis:6379

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Queue Management System
VERSION=1.0.0

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://frontend:3000,http://localhost,http://127.0.0.1

# Debug Mode
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Gemini AI Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Cloudinary Configuration
# Get credentials from: https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000/ws


